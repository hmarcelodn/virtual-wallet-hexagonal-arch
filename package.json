{
   "name": "agileengine-bootcamp",
   "version": "1.0.0",
   "description": "AgileEngine Bootcamp",
   "main": "index.js",
   "scripts": {
      "commit": "git-cz",
      "start:dev": "cross-env NODE_ENV=development nodemon --watch src --delay 1 --exec 'ts-node' -r dotenv/config src/server.ts",
      "start": "ts-node src/server.ts",
      "debug": "cross-env NODE_ENV=development nodemon --watch src --delay 1 --exec node --inspect-brk -r ts-node/register src/server.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "apidoc": "apidoc -i src/ -o ./public/apidoc/",
      "lint": "eslint . --ext ts",
      "lint:fix": "eslint . --ext ts --fix",
      "format": "prettier --write 'src/**/*.{ts,css,md,json}' --config ./.prettierrc"
   },
   "apidoc": {
      "name": "AE Bootcamp",
      "title": "AE Bootcamp",
      "url": "https://api.github.com/v1"
   },
   "husky": {
      "hooks": {
         "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
         "pre-commit": "npm run lint"
      }
   },
   "config": {
      "commitizen": {
         "path": "cz-conventional-changelog"
      }
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/hmarcelodn-ae/agileengine-bootcamp.git"
   },
   "keywords": [],
   "author": "Hugo Marcelo Del Negro",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/hmarcelodn-ae/agileengine-bootcamp/issues"
   },
   "homepage": "https://github.com/hmarcelodn-ae/agileengine-bootcamp#readme",
   "dependencies": {
      "axios": "^0.22.0",
      "compression": "^1.7.4",
      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "cross-env": "^7.0.3",
      "crypto-js": "^4.1.1",
      "dotenv": "^16.0.1",
      "ejs": "^3.1.6",
      "express": "^4.17.1",
      "express-async-errors": "^3.1.1",
      "express-validator": "^6.12.2",
      "jsonwebtoken": "^8.5.1",
      "lodash": "^4.17.21",
      "mysql": "^2.14.1",
      "node-cron": "^3.0.0",
      "password-validator": "^5.1.1",
      "pg": "^8.7.1",
      "reflect-metadata": "^0.1.10",
      "tsconfig-paths": "^3.11.0",
      "typedi": "^0.10.0",
      "typeorm": "^0.3.7"
   },
   "devDependencies": {
      "@commitlint/cli": "^13.2.0",
      "@commitlint/config-conventional": "^13.2.0",
      "@commitlint/prompt-cli": "^13.2.0",
      "@types/axios": "^0.14.0",
      "@types/compression": "^1.7.2",
      "@types/cookie-parser": "^1.4.2",
      "@types/cors": "^2.8.12",
      "@types/crypto-js": "^4.0.2",
      "@types/ejs": "^3.1.0",
      "@types/express": "^4.17.13",
      "@types/express-validator": "^3.0.0",
      "@types/jsonwebtoken": "^8.5.5",
      "@types/lodash": "^4.14.175",
      "@types/node": "^8.0.29",
      "@types/node-cron": "^2.0.4",
      "@typescript-eslint/eslint-plugin": "^5.33.1",
      "@typescript-eslint/parser": "^5.33.1",
      "cz-conventional-changelog": "^3.3.0",
      "eslint": "^8.22.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-config-standard-with-typescript": "^22.0.0",
      "eslint-import-resolver-typescript": "^3.4.1",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-n": "^15.2.4",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-promise": "^6.0.0",
      "git-cz": "^4.7.6",
      "husky": "^7.0.2",
      "nodemon": "^2.0.13",
      "prettier": "^2.7.1",
      "ts-node": "^10.9.1",
      "tslint": "^6.1.3",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "^4.7.4"
   }
}
